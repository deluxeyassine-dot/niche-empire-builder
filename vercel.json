{
  "version": 2,
  "buildCommand": "echo FORCE DEPLOY v9 - Simple Video Gen - $(date) && ls -la public/*.html | wc -l",
  "outputDirectory": "public",
  "framework": null,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ]
}
